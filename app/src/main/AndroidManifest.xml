<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chris.sesi.com.view">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MANAGE_DOCUMENTS" />
    <uses-permission android:name="com.google.android.apps.photos.permission.GOOGLE_PHOTOS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:fullBackupContent="@xml/backup_descriptor">
        <activity
            android:name="chris.sesi.com.view.activity.MainActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize|stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="chris.sesi.com.view.activity.Registro"
            android:label="@string/title_activity_registro"
            android:parentActivityName="chris.sesi.com.view.activity.MenuPrincipal"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize|stateHidden">

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="chris.sesi.com.view.activity.MenuPrincipal" />
        </activity>
        <activity
            android:name="chris.sesi.com.view.activity.MenuPrincipal"
            android:label="@string/title_activity_menu_principal"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="chris.sesi.com.view.activity.FormNuevoCliente"
            android:label="@string/title_activity_form_nuevo_cliente"
            android:parentActivityName="chris.sesi.com.view.activity.MenuPrincipal"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="adjustResize|stateHidden">

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="chris.sesi.com.view.activity.MenuPrincipal" />
        </activity>
        <activity
            android:name="chris.sesi.com.view.activity.ClientList"
            android:label="@string/title_activity_client_list"
            android:parentActivityName="chris.sesi.com.view.activity.MenuPrincipal"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="chris.sesi.com.view.activity.MenuPrincipal" />
        </activity>

        <activity
            android:name="chris.sesi.com.view.activity.Catalogo"
            android:label="@string/title_activity_catalogo"
            android:parentActivityName="chris.sesi.com.view.activity.MenuPrincipal"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="chris.sesi.com.view.activity.MenuPrincipal" />
        </activity>
        <activity
            android:name="chris.sesi.com.view.activity.Inventario"
            android:label="@string/title_activity_inventario"
            android:parentActivityName="chris.sesi.com.view.activity.MenuPrincipal"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="chris.sesi.com.view.activity.MenuPrincipal" />
        </activity>
        <activity
            android:name="chris.sesi.com.view.activity.FormAgregarStock"
            android:label="@string/title_activity_form_agregar_stock"
            android:parentActivityName="chris.sesi.com.view.activity.Inventario"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="adjustResize|stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="chris.sesi.com.view.activity.Inventario" />
        </activity>
        <activity
            android:name="chris.sesi.com.view.activity.Venta"
            android:label="@string/title_activity_venta"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity
            android:name="chris.sesi.com.view.activity.FormNuevaConsultora"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity
            android:name="chris.sesi.com.view.activity.ConsultoraList"
            android:label="@string/title_activity_consultora_list"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="chris.sesi.com.view.activity.MenuPrincipal" />
        </activity>
        <activity
            android:name="chris.sesi.com.view.activity.UserProfile"
            android:label="@string/title_activity_user_profile"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity
            android:name="chris.sesi.com.view.activity.MiUnidad"
            android:label="@string/title_activity_mi_unidad"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>